{
  "generatedComponents": [
    {
      "schema_description": "React application scaffold for a property booking platform with host and guest workflows",
      "elements": {
        "description": "Atomic UI primitives for the booking platform",
        "items": {
          "Button": {
            "description": "Primary action button with variants for different states"
          },
          "Input": {
            "description": "Text input field with validation states"
          },
          "Select": {
            "description": "Dropdown selector for options"
          },
          "Checkbox": {
            "description": "Checkbox input for boolean selections"
          },
          "Textarea": {
            "description": "Multi-line text input for descriptions"
          },
          "Slider": {
            "description": "Range slider for price filtering"
          },
          "Card": {
            "description": "Container card for displaying content"
          },
          "Badge": {
            "description": "Small status indicator or tag"
          },
          "Avatar": {
            "description": "User profile image placeholder"
          },
          "Label": {
            "description": "Form field labels"
          }
        }
      },
      "modules": {
        "description": "Reusable stateless components composed from elements",
        "items": {
          "SearchFilters": {
            "description": "Filter controls for listing search including location, price range, and guest count",
            "composition": {
              "elements": [
                "Input",
                "Slider",
                "Select",
                "Label",
                "Button"
              ],
              "modules": []
            }
          },
          "ListingCard": {
            "description": "Display card for individual listing with image, title, location, and price",
            "composition": {
              "elements": [
                "Card",
                "Badge"
              ],
              "modules": []
            }
          },
          "ListingForm": {
            "description": "Form for creating new listings with all required fields",
            "composition": {
              "elements": [
                "Input",
                "Textarea",
                "Select",
                "Checkbox",
                "Button",
                "Label"
              ],
              "modules": []
            }
          },
          "AmenitiesSelector": {
            "description": "Checklist of available amenities for listings",
            "composition": {
              "elements": [
                "Checkbox",
                "Label"
              ],
              "modules": []
            }
          },
          "BookingRequestCard": {
            "description": "Card displaying booking request details for host approval",
            "composition": {
              "elements": [
                "Card",
                "Button",
                "Badge",
                "Avatar"
              ],
              "modules": []
            }
          }
        }
      },
      "sections": {
        "description": "Smart components with data fetching and business logic",
        "items": {
          "ListingSearchSection": {
            "description": "Complete search interface with filters and results display",
            "composition": {
              "modules": [
                "SearchFilters",
                "ListingCard"
              ]
            },
            "data_requirements": [
              {
                "type": "query",
                "description": "Search for available listings based on filters",
                "trigger": "on_filter_change",
                "details": {
                  "source": "listing_service",
                  "gql": "query SearchListings($location: String, $maxPrice: Float, $minGuests: Int) { searchListings(location: $location, maxPrice: $maxPrice, minGuests: $minGuests) { propertyId title location pricePerNight maxGuests } }",
                  "payload_schema": {
                    "location": "String",
                    "maxPrice": "Float",
                    "minGuests": "Int"
                  }
                }
              }
            ]
          },
          "CreateListingSection": {
            "description": "Form section for hosts to create new listings",
            "composition": {
              "modules": [
                "ListingForm",
                "AmenitiesSelector"
              ]
            },
            "data_requirements": [
              {
                "type": "mutation",
                "description": "Create a new listing",
                "trigger": "on_form_submit",
                "details": {
                  "source": "listing_service",
                  "gql": "mutation CreateListing($input: CreateListingInput!) { createListing(input: $input) { propertyId title location } }",
                  "payload_schema": {
                    "title": "String",
                    "description": "String",
                    "location": "String",
                    "address": "String",
                    "pricePerNight": "Float",
                    "maxGuests": "Int",
                    "amenities": "[String]"
                  }
                }
              }
            ]
          },
          "BookingRequestsSection": {
            "description": "Section for hosts to view and manage booking requests",
            "composition": {
              "modules": [
                "BookingRequestCard"
              ]
            }
          }
        }
      },
      "pages": {
        "description": "Top-level page views with routing and navigation",
        "items": {
          "SearchPage": {
            "route": "/search",
            "description": "Main search page for guests to find and book listings",
            "layout": {
              "sections": [
                "ListingSearchSection"
              ],
              "modules": [],
              "elements": []
            },
            "navigation": [
              {
                "on": "listing_click",
                "to": "/listing/:id"
              }
            ],
            "data_requirements": [
              {
                "type": "query",
                "description": "Load initial listings on page mount",
                "trigger": "on_page_load",
                "details": {
                  "source": "listing_service",
                  "gql": "query SearchListings($location: String, $maxPrice: Float, $minGuests: Int) { searchListings(location: $location, maxPrice: $maxPrice, minGuests: $minGuests) { propertyId title location pricePerNight maxGuests } }"
                }
              }
            ]
          },
          "CreateListingPage": {
            "route": "/host/create-listing",
            "description": "Page for hosts to create new property listings",
            "layout": {
              "sections": [
                "CreateListingSection"
              ],
              "modules": [],
              "elements": []
            },
            "navigation": [
              {
                "on": "listing_created",
                "to": "/host/dashboard"
              }
            ],
            "data_requirements": [
              {
                "type": "mutation",
                "description": "Submit new listing data",
                "trigger": "on_form_submit",
                "details": {
                  "source": "listing_service",
                  "gql": "mutation CreateListing($input: CreateListingInput!) { createListing(input: $input) { propertyId title location } }"
                }
              }
            ]
          },
          "HostDashboard": {
            "route": "/host/dashboard",
            "description": "Host dashboard showing booking requests and listing management",
            "layout": {
              "sections": [
                "BookingRequestsSection"
              ],
              "modules": [],
              "elements": []
            },
            "navigation": [
              {
                "on": "create_listing_click",
                "to": "/host/create-listing"
              }
            ]
          }
        }
      }
    }
  ],
  "layout": {
    "navigation": {
      "primary": [
        {
          "label": "Search",
          "route": "/search",
          "role": "guest"
        },
        {
          "label": "Host Dashboard",
          "route": "/host/dashboard",
          "role": "host"
        }
      ],
      "secondary": [
        {
          "label": "Create Listing",
          "route": "/host/create-listing",
          "role": "host"
        }
      ]
    },
    "pageFlow": {
      "guest": [
        "/search",
        "/listing/:id",
        "/booking/request"
      ],
      "host": [
        "/host/dashboard",
        "/host/create-listing"
      ]
    },
    "responsive": {
      "mobile": {
        "breakpoint": "768px",
        "adaptations": [
          "stack_filters",
          "single_column_results"
        ]
      },
      "tablet": {
        "breakpoint": "1024px",
        "adaptations": [
          "sidebar_filters",
          "grid_results"
        ]
      }
    }
  }
}