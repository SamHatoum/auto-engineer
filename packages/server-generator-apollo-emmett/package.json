{
  "name": "@auto-engineer/server-generator-apollo-emmett",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "generate:server": "tsx src/cli/index.ts",
    "build": "tsc && tsx ../../scripts/fix-esm-imports.ts && rm -rf dist/codegen/templates && mkdir -p dist/codegen && cp -r src/codegen/templates dist/codegen/templates && cp src/server.ts dist/ && cp -r src/utils dist/ && cp -r src/domain dist/",
    "dev": "tsc --watch",
    "test": "vitest run --reporter=dot",
    "type-check": "tsc --noEmit --project tsconfig.json",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.specs.ts' --ignore-pattern '**/.tmp/**' --max-warnings 0 --config ../../eslint.config.ts",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore --log-level warn",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern '**/*.specs.ts' --ignore-pattern '**/.tmp/**' --fix --config ../../eslint.config.ts",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore --log-level warn",
    "link:dev": "pnpm build && pnpm link --global",
    "unlink:dev": "pnpm unlink --global",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@auto-engineer/flow": "workspace:*",
    "@auto-engineer/message-bus": "workspace:*",
    "@event-driven-io/emmett": "^0.38.2",
    "apollo-server": "^3.13.0",
    "apollo-server-express": "^3.13.0",
    "change-case": "^5.4.4",
    "ejs": "^3.1.10",
    "execa": "^9.6.0",
    "express": "^4.21.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "graphql": "^16.11.0",
    "graphql-scalars": "^1.24.2",
    "prettier": "^3.6.1",
    "reflect-metadata": "^0.2.2",
    "type-fest": "^4.41.0",
    "type-graphql": "^2.0.0-rc.2",
    "uuid": "^10.0.0",
    "web-streams-polyfill": "^4.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@auto-engineer/cli": "workspace:*",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4"
  },
  "version": "0.8.1"
}
