{
  "name": "@auto-engineer/message-store",
  "type": "module",
  "version": "0.9.3",
  "description": "Message store for commands and events with pluggable backends",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "scripts": {
    "build": "tsc && tsx ../../scripts/fix-esm-imports.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --reporter=dot",
    "lint": "eslint 'src/**/*.ts' --max-warnings 0 --config ../../eslint.config.ts",
    "lint:fix": "eslint 'src/**/*.ts' --fix --config ../../eslint.config.ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore --log-level warn",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore --log-level warn",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@auto-engineer/message-bus": "workspace:*",
    "debug": "^4.3.4",
    "nanoid": "^5.0.0"
  },
  "devDependencies": {
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
