{
  "name": "@auto-engineer/cli",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/dsl-exports.d.ts",
      "import": "./dist/src/dsl-exports.js",
      "require": "./dist/src/dsl-exports.js",
      "default": "./dist/src/dsl-exports.js"
    },
    "./manifest-types": {
      "types": "./dist/src/manifest-types.d.ts",
      "import": "./dist/src/manifest-types.js"
    },
    "./utils/get-package-version": {
      "types": "./dist/src/utils/get-package-version.d.ts",
      "import": "./dist/src/utils/get-package-version.js"
    }
  },
  "bin": {
    "auto-engineer": "./dist/bin/auto.js",
    "auto": "./dist/bin/auto.js"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc && tsx ../../scripts/fix-esm-imports.ts && chmod +x dist/bin/auto.js && cp -r src/server/*.html src/server/*.svg dist/src/server/ 2>/dev/null || true",
    "test": "vitest run --reporter=dot",
    "test:watch": "vitest",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.specs.ts' --max-warnings 0 --config ../../eslint.config.ts",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore --log-level warn",
    "lint:fix": "eslint 'src/**/*.ts' --fix --config ../../eslint.config.ts",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore --log-level warn",
    "link:dev": "pnpm build && pnpm link --global",
    "unlink:dev": "pnpm unlink --global"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auto-engineer/file-store": "workspace:*",
    "@auto-engineer/message-bus": "workspace:*",
    "@auto-engineer/message-store": "workspace:*",
    "@scarf/scarf": "^1.4.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "commander": "^12.0.0",
    "cors": "^2.8.5",
    "debug": "^4.4.1",
    "dotenv": "^16.4.7",
    "execa": "^9.6.0",
    "express": "^4.18.0",
    "figlet": "^1.7.0",
    "get-port": "^7.1.0",
    "gradient-string": "^3.0.0",
    "inquirer": "^9.2.15",
    "jiti": "^2.5.1",
    "kleur": "^4.1.5",
    "marked": "^15.0.12",
    "marked-terminal": "^7.3.0",
    "nanoid": "^5.0.0",
    "ora": "^8.0.1",
    "socket.io": "^4.7.5",
    "test": "^3.3.0",
    "tree-kill": "^1.2.2",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^9.0.7",
    "@types/js-yaml": "^4.0.9",
    "@types/marked": "^6.0.0",
    "@types/marked-terminal": "^6.1.1",
    "@types/node": "^22.0.0",
    "node-fetch": "^3.3.2",
    "tsx": "^4.20.3"
  },
  "version": "0.11.15"
}
