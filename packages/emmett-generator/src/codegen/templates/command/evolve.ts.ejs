<% if (events.length) { -%>
    import type { State } from './state';
    <% events.forEach(event => { -%>
        import type { <%= pascalCase(event.type) %> } from './events';
    <% }); -%>

    export const evolve = (
    state: State,
    event: <%= events.map(e => pascalCase(e.type)).join(' | ') %>
    ): State => {
    switch (event.type) {
    <% events.forEach(event => { -%>
        case '<%= event.type %>':
        return {
        ...state,
        ...event.data,
        };
    <% }); -%>
    default:
    return state;
    }
    };
<% } else { -%>
    // No events defined for this slice. Evolve logic will be generated once events exist.
<% } -%>