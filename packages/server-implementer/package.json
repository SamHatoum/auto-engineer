{
  "name": "@auto-engineer/server-implementer",
  "version": "0.4.8",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "implement-server": "./dist/cli/implement-server.js"
  },
  "scripts": {
    "implement": "tsx src/cli/index.ts",
    "implement:server": "tsx src/cli/implement-server.ts",
    "build": "tsc && tsx ../../scripts/fix-esm-imports.ts && chmod +x dist/cli/implement-server.js",
    "dev": "tsc --watch",
    "test": "vitest run",
    "lint": "eslint 'src/**/*.ts' --max-warnings 0 --config ../../eslint.config.ts",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore",
    "lint:fix": "eslint 'src/**/*.ts' --fix --config ../../eslint.config.ts",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path ../../.prettierignore",
    "link:dev": "pnpm build && pnpm link --global",
    "unlink:dev": "pnpm unlink --global",
    "prepublishOnly": "npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@auto-engineer/ai-gateway": "^0.4.5",
    "@auto-engineer/message-bus": "^0.3.4",
    "@modelcontextprotocol/sdk": "^1.3.0",
    "debug": "^4.3.4",
    "fast-glob": "^3.3.3",
    "vite": "^5.4.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "dotenv": "^16.6.1",
    "execa": "^9.6.0",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  }
}
