{
  "name": "auto-engineer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path .prettierignore",
    "check": "turbo run lint format type-check test",
    "clean": "tsx scripts/clean.ts",
    "reset": "pnpm clean && pnpm install && pnpm build",
    "link:dev": "pnpm --filter @auto-engineer/* run link:dev",
    "unlink:dev": "pnpm --filter @auto-engineer/* run unlink:dev",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "prepare": "husky",
    "install": "turbo run install build",
    "clean:auto": "node scripts/clean-global-auto.js",
    "## ----------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------": "",
    "start:ecommerce-site": "cd examples/ecommerce-site && pnpm start",
    "start:product-api": "cd examples/product-catalogue-api && pnpm start",
    "start:cart-api": "cd examples/cart-api && pnpm start",
    "start:examples": "pnpm --parallel --filter @examples/* start 2>&1 | grep -E '(localhost|Local)' || true"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.2.1",
    "@eslint/js": "^9.30.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.30.1",
    "husky": "^9.0.11",
    "jiti": "^2.5.0",
    "playwright": "^1.54.1",
    "prettier": "^3.3.3",
    "tsx": "^4.7.1",
    "turbo": "^2.5.5",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.13.1",
  "resolutions": {
    "punycode": "^2.3.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7"
  },
  "dependencies": {
    "@auto-engineer/ai-gateway": "link:packages/ai-gateway",
    "@auto-engineer/flowlang": "link:packages/flowlang",
    "axios": "^1.10.0",
    "debug": "^4.4.1",
    "dotenv": "^16.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "pnpm format:fix",
      "git add"
    ]
  }
}
