{
  "name": "auto-engineer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "format": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path .prettierignore",
    "format:check": "prettier --check \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path .prettierignore",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path .prettierignore",
    "check": "turbo run lint type-check test format:check",
    "clean": "tsx scripts/clean.ts",
    "reset": "pnpm clean && pnpm install && pnpm build",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "prepare": "husky",
    "install": "turbo run install",
    "start": "pnpm --filter @auto-engineer/cli start",
    "information-architect": "pnpm --filter @auto-engineer/information-architect generate-ia-schema ../../../example-booking-app ../../examples/flows/src/shopping-assistant.flow.ts",
    "react-generator": "pnpm --filter @auto-engineer/react-graphql-generator start  ../../examples/flows/src/shopping-assistant.flow.ts && cd ../example-booking-app && yarn",
    "frontend-implementation": "pnpm --filter @auto-engineer/frontend-implementation ai-agent ../../../example-booking-app",
    "fe": "pnpm information-architect && pnpm react-generator && pnpm frontend-implementation"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.2.1",
    "@eslint/js": "^9.30.1",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.30.1",
    "husky": "^9.0.11",
    "prettier": "^3.3.3",
    "tsx": "^4.7.1",
    "turbo": "^2.5.4",
    "typescript": "~5.5.0",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.12.1",
  "resolutions": {
    "punycode": "^2.3.1"
  },
  "dependencies": {
    "dotenv": "^16.4.5"
  }
}
