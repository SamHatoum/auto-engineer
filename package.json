{
  "name": "auto-engineer",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter='./packages/*' --filter='./examples/*' --concurrency=20",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "type-check": "turbo run type-check",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "format:fix": "prettier --write \"**/*.{js,ts,json,md,yml,yaml}\" --ignore-path .prettierignore",
    "check": "turbo run lint format type-check test",
    "clean": "tsx scripts/clean.ts",
    "reset": "pnpm clean && pnpm install && pnpm build",
    "link:dev": "pnpm --filter @auto-engineer/* run link:dev",
    "unlink:dev": "pnpm --filter @auto-engineer/* run unlink:dev",
    "changeset": "changeset",
    "changeset:prepare-changelog": "tsx scripts/prepare-changelog.ts",
    "version": "changeset version && pnpm changeset:prepare-changelog",
    "release": "changeset publish",
    "prepare": "husky",
    "install": "turbo run install build",
    "update:auto": "node scripts/clean-global-auto.js && rm -rf $(pnpm root -g) && pnpm i -g @auto-engineer/cli@latest",
    "kill:auto": "lsof -ti:5555 | xargs kill -9 2>/dev/null"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.27.1",
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@commitlint/types": "^19.2.1",
    "@eslint/js": "^9.30.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "@vitest/coverage-v8": "^3.2.3",
    "eslint": "^9.30.1",
    "husky": "^9.0.11",
    "jiti": "^2.5.0",
    "playwright": "^1.54.1",
    "prettier": "^3.3.3",
    "tsx": "^4.7.1",
    "turbo": "^2.5.6",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vitest": "^3.2.3"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.15.0",
  "resolutions": {
    "punycode": "^2.3.1",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7"
  },
  "dependencies": {
    "@auto-engineer/ai-gateway": "^0.4.0",
    "@auto-engineer/design-system-importer": "link:../../../Library/pnpm/global/5/node_modules/@auto-engineer/design-system-importer",
    "@auto-engineer/flow": "^0.1.1",
    "axios": "^1.10.0",
    "debug": "^4.4.1",
    "dotenv": "^16.4.5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "pnpm format:fix",
      "git add"
    ]
  }
}
